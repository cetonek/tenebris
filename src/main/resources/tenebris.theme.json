{
  "name": "Tenebris",
  "dark": true,
  "author": "cetonek",
  "editorScheme": "/tenebris.xml",
  "colors": {
    "black28": "#1C1C1C",
    "black43": "#2b2b2b",
    "black49": "#313131",
    "black52": "#343434",
    "black76": "#4c4c4c",
    "white213": "#d5d5d5",
    "backgroundSelectedItem": "#1c396f"
  },
  "ui": {
    "*": {
      "background": "black28",
      "selectionBackground": "backgroundSelectedItem",
      "foreground": "white213",
      "borderColor": "black43",
      "separatorColor": "black43",
      "selectedBackground": "black52",
      "focusedBorderColor": "backgroundSelectedItem"
    },
    "Button": {
      "startBackground": "black52",
      "endBackground": "black52",
      "startBorderColor": "black52",
      "endBorderColor": "black52",
      "disabledBorderColor": "black52",
      "default": {
        "startBackground": "black76",
        "endBackground": "black76",
        "startBorderColor": "black52",
        "endBorderColor": "black52"
      }
    },
    "ActionButton": {
      "hoverBackground": "black52",
      "hoverBorderColor": "black52",
      "pressedBackground": "black76",
      "pressedBorderColor": "black76"
    },
    "Borders": {
      "color": "black43",
      "ContrastBorderColor": "black43"
    },
    "DefaultTabs": {
      "hoverBackground": "black52"
    },
    "EditorTabs": {
      "underlinedTabBackground": "black52"
    },
    "ToolWindow": {
      "Button": {
        "hoverBackground": "black52"
      },
      "Header": {
        "inactiveBackground": "black28"
      }
    },
    "VersionControl": {
      "Log.Commit": {
        "currentBranchBackground": "black28",
        "hoveredBackground": "black52"
      }
    },
    "ScrollBar": {
      "thumbColor": "black49",
      "Transparent.thumbColor": "black49",
      "Mac.thumbColor": "black49"
    },
    "Popup": {
      "inactiveBorderColor": "black52",
      "Header.activeBackground": "black52",
      "Header.inactiveBackground": "black52"
    },
    "MemoryIndicator": {
      "usedBackground": "black52",
      "allocatedBackground": "black52"
    },
    "ToolTip": {
      "background": "black52"
    },
    "Table": {
      "gridColor": "black43"
    },
    "CompletionPopup": {
      "selectionInactiveBackground": "backgroundSelectedItem"
    },
    "ProgressBar": {
      "trackColor": "black52",
      "progressColor": "black76",
      "indeterminateStartColor": "black52",
      "indeterminateEndColor": "black76"
    },
    "FileColor": {
      "Yellow": "#30311e",
      "Green": "#1e311e",
      "Blue": "#1e2531",
      "Violet": "#2e1e31",
      "Orange": "#31291e",
      "Rose": "#311e2a"
    }
  }
}
