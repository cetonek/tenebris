{
  "name": "Tenebris",
  "dark": true,
  "author": "cetonek",
  "editorScheme": "/tenebris.xml",
  "colors": {
    "text": "#d5d5d5",
    "backgroundColor": "#1C1C1C",
    "backgroundFocused": "#343434",
    "backgroundSelectedItem": "#1c396f",
    "borderColor": "#2b2b2b",
    "scrollBarThumb": "#313131",
    "dialogHeaderBackground": "#343434",
    "dialogBorder": "#343434",
    "buttonBorder": "#343434"
  },
  "ui": {
    "*": {
      "background": "backgroundColor",
      "selectionBackground": "backgroundSelectedItem",
      "foreground": "text",
      "borderColor": "borderColor",
      "separatorColor": "borderColor",
      "selectedBackground": "backgroundFocused",
      "focusedBorderColor": "backgroundSelectedItem"
    },
    "Button": {
      "startBackground": "backgroundFocused",
      "endBackground": "backgroundFocused",
      "startBorderColor": "buttonBorder",
      "endBorderColor": "buttonBorder",
      "disabledBorderColor" : "buttonBorder",
      "default": {
        "startBackground": "#4c4c4c",
        "endBackground": "#4c4c4c",
        "startBorderColor": "buttonBorder",
        "endBorderColor": "buttonBorder"
      }
    },
    "ActionButton": {
      "hoverBackground": "backgroundFocused",
      "hoverBorderColor": "backgroundFocused",
      "pressedBackground": "#4c4c4c",
      "pressedBorderColor": "#4c4c4c"
    },
    "Borders": {
      "color": "borderColor",
      "ContrastBorderColor": "borderColor"
    },
    "DefaultTabs": {
      "hoverBackground": "backgroundFocused"
    },
    "EditorTabs": {
      "underlinedTabBackground": "backgroundFocused"
    },
    "ToolWindow": {
      "Button": {
        "hoverBackground": "backgroundFocused"
      },
      "Header": {
        "inactiveBackground": "backgroundColor"
      }
    },
    "VersionControl": {
      "Log.Commit": {
        "currentBranchBackground": "backgroundColor",
        "hoveredBackground": "backgroundFocused"
      }
    },
    "ScrollBar": {
      "thumbColor": "scrollBarThumb",
      "Transparent.thumbColor": "scrollBarThumb",
      "Mac.thumbColor": "scrollBarThumb"
    },
    "Popup": {
      "inactiveBorderColor": "dialogBorder",
      "Header.activeBackground": "dialogHeaderBackground",
      "Header.inactiveBackground": "dialogHeaderBackground"
    },
    "MemoryIndicator": {
      "usedBackground": "backgroundFocused",
      "allocatedBackground": "backgroundFocused"
    },
    "ToolTip": {
      "background": "backgroundFocused"
    },
    "Table": {
      "gridColor": "borderColor"
    },
    "CompletionPopup": {
      "selectionInactiveBackground": "backgroundSelectedItem"
    },
    "ProgressBar": {
      "trackColor": "backgroundFocused",
      "progressColor": "#4c4c4c",
      "indeterminateStartColor": "backgroundFocused",
      "indeterminateEndColor": "#4c4c4c"
    },
    "FileColor": {
      "Yellow": "#30311e",
      "Green": "#1e311e",
      "Blue": "#1e2531",
      "Violet": "#2e1e31",
      "Orange": "#31291e",
      "Rose": "#311e2a"
    }
  }
}
